\mode<presentation>

\definecolor{unipdcrimson}{rgb}{0.6078,0.0,0.078}
\definecolor{unipdgrey2}{rgb}{0.6156,0.6353,0.6824}
\definecolor{unizarblack}{rgb}{0.09412,0.08235,0.07059}
\definecolor{unipdlightgrey}{rgb}{0.9647,0.9608,0.9568}
\definecolor{unipddarkgrey}{rgb}{0.27058,0.2863,0.3255}

\setbeamercolor*{palette primary}{bg=unipdcrimson,fg=unipdlightgrey}
\setbeamercolor*{palette secondary}{bg=unipdgrey2, fg=unipdcrimson}
\setbeamercolor*{palette tertiary}{bg=unipddarkgrey, fg=unipdlightgrey}
\setbeamercolor*{palette quaternary}{bg=unipdlightgrey, fg=unipddarkgrey}
\setbeamercolor*{item}{fg=unipdcrimson} 
\setbeamercolor*{title in head/foot}{bg=unipdcrimson, fg=unipdlightgrey}
\setbeamercolor*{date in head/foot}{bg=unipddarkgrey, fg=unipdlightgrey}
\setbeamercolor*{author in head/foot}{bg=unipddarkgrey, fg=unipdlightgrey}
\setbeamercolor*{number in head/foot}{bg=unipddarkgrey, fg=unipdlightgrey}
\setbeamercolor*{frametitle}{bg=unipdcrimson,fg=unipdlightgrey}
\setbeamercolor*{normal text}{fg=unizarblack, bg=unipdlightgrey}
\setbeamercolor*{alerted text}{fg=unipdcrimson, bg=unipdlightgrey}
\setbeamercolor*{section in toc}{fg=unipddarkgrey}
\setbeamercolor*{caption name}{fg=unipddarkgrey}
\setbeamercolor*{block body}{fg=unizarblack, bg=unipdgrey2}
\setbeamercolor*{block title}{fg=unipdlightgrey, bg=unipdcrimson}

\mode
<all>
